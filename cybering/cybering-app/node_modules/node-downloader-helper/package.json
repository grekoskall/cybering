{
  "name": "node-downloader-helper",
  "version": "1.0.8",
  "description": "A simple http file downloader for node.js",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./example/index.js",
    "lint": "./node_modules/.bin/eslint src",
    "build": "./node_modules/.bin/babel --presets env ./src/index.js -o ./dist/index.js",
    "watch": "./node_modules/.bin/babel --watch --presets env ./src/index.js -o ./dist/index.js",
    "test": "npm run build && npm run lint && \"node_modules/.bin/mocha\""
  },
  "keywords": [
    "node",
    "nodejs",
    "node.js",
    "download",
    "resumable",
    "resume",
    "http"
  ],
  "author": "Jose De Gouveia <dhgouveia@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hgouveia/node-downloader-helper"
  },
  "bugs": {
    "url": "https://github.com/hgouveia/node-downloader-helper/issues"
  },
  "homepage": "https://github.com/hgouveia/node-downloader-helper",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "mocha": "^5.1.1"
  }
}